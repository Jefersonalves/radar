# .coveragerc to control coverage.py
[run]
omit =
    # omit anything in test directory anywhere
    */test/*
    # omit anything in tests directory anywhere
    */tests/*
    # omit anything in testes_integracao directory
    testes_integracao/*
    # omit anything in util_test directory
    util_test/*
    manage.py
    */migrations/*
